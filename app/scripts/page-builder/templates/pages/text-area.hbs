<div id="box1">
	<div id="wrapper">
        <div id="content" align="left">
            <a name="content"></a>
            <h3>{{title}}</h3>
            {{{description}}}
            <form name="form1" action="">
                <fieldset>
                    <legend>
                        <span class="instruction">{{{instructions}}}</span>
                    </legend>
                    <strong>
                        <label for="compareMe">{{{compareMessage}}}</label>
                    </strong>
                    <div class="centeredText">
                        <textarea id="compareMe" cols="70" rows="8" name="response" onfocus="if(document.form1.response.value=='Type your answer in this area.'){document.form1.response.value=''}">Type your answer in this area.</textarea><br />
                        <a href="javascript:ScoreMe()" onmouseover="document.submitme.src='images/buttonSubmitHi.gif';" onmouseout="document.submitme.src='images/buttonSubmit.gif';" onmousedown="document.submitme.src='images/buttonSubmitDn.gif';"><img src="images/buttonSubmit.gif" alt="Submit your answer" name="submitme" border="0" id="submitme" /></a>
                    </div>
                </fieldset>
                <div id="feedbackDiv"></div>
            </form>
            <script language="JavaScript" src="scripts/FeedbackCompare.js" type="text/javascript"></script>
            <script language="JavaScript" type="text/javascript">
                var FBheader = '"{{{feedbackHeader}}}"' // Feel free to edit this text
                var Feedback = '{{{feedback}}}'
            </script>
        </div><!-- end #content -->
	</div><!-- end #wrapper -->
</div><!-- end #box1 -->
